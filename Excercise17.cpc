DECLARE UserNumber1: INTEGER
DECLARE UserNumber2:INTEGER
FUNCTION Max(Number1 : INTEGER, Number2 : INTEGER) RETURNS INTEGER
  IF Number1 > Number2 THEN
    RETURN Number1
  ELSE
    RETURN Number2
  ENDIF
ENDFUNCTION

OUTPUT "please input the first Number:"
INPUT UserNumber1
OUTPUT "please input the second Number:"
INPUT UserNumber2
OUTPUT "The Biggest Number Is", Max(UserNumber1,UserNumber2)